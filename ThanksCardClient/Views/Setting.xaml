<UserControl x:Class="ThanksCardClient.Views.Setting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="IMG_1154.JPG" Opacity="0.5"/>
        </Grid.Background>
        <Button Content="戻る" Command="{Binding BackCommand}" Height="40" Margin="0,377,0,0" Background="#FFAECFFF" HorizontalAlignment="Right" Width="101.147" />
        <Button Content="社員情報の編集" Command="{Binding UserCommand}" HorizontalAlignment="Center" Height="45" Margin="78,44,0,0" VerticalAlignment="Top" Width="444" FontSize="16" Background="#FFAECFFF"/>
        <Button Content="部署情報の編集" Command="{Binding DepartmentCreateCommand}" HorizontalAlignment="Center" Height="45" Margin="78,120.5,0,0" VerticalAlignment="Top" Width="444" FontSize="16" Background="#FFAECFFF"/>
        <Button Content="カテゴリの編集" Command="{Binding TagMstCommand}" HorizontalAlignment="Center" Height="45" Margin="78,280.5,0,0" VerticalAlignment="Top" Width="444" FontSize="16" Background="#FFAECFFF"/>
        <Button Content="課情報の編集" Command="{Binding DepartmentChildrenCreateCommand}" HorizontalAlignment="Center" Height="45" Margin="78,203.5,0,0" VerticalAlignment="Top" Width="444" FontSize="16" Background="#FFAECFFF"/>
    </Grid>
</UserControl>